<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName">

    <bean class="com.ym.materials.customProxyFactoryBean.RevokerFactoryBean" lazy-init="false">
        <property name="targetInterface" value="com.ym.materials.customProxyFactoryBean.HelloService">
        </property>
        <property name="clusterStrategy" value="WeightRandom"/>
        <property name="remoteAppKey" value="ares"/>
        <property name="groupName" value="default"/>
        <property name="timeout" value="600"/>
    </bean>
</beans>